{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Keycloak": {
    "CONTAINER_NAME": "keycloak",
    "IMAGE": "quay.io/keycloak/keycloak:latest",
    "ARGS": [ "start-dev" ],
    "KEYCLOAK_ADMIN": "admin",
    "KEYCLOAK_ADMIN_PASSWORD": "admin",
    "KC_DB": "postgres",
    "KC_DB_URL": "jdbc:postgresql://postgres:5432/keycloak-db",
    "KC_DB_USERNAME": "postgres",
    "KC_DB_PASSWORD": "Postgres10",
    "HOST_PORT": 8080,
    "CONTAINER_PORT": 8080,
    "Providers": [
      "D:\\Personal Projects\\keycloak-spis\\keycloak-spis\\spi-policy\\target\\spi-policy-1.0.0.jar"
    ]
  },
  "KeycloakDatabase": {
    "ContainerName": "keycloak-postgres",
    "Image": "postgres:16-alpine",
    "User": "postgres",
    "Password": "Postgres10",
    "DatabaseName": "keycloak-db",
    "HostPort": 5432,
    "ContainerPort": 5432,
    "VolumePath": "/var/lib/postgresql/data",
    "URL": "locahost"
  }
}
