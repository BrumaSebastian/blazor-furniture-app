{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "Keycloak": {
    "ContainerName": "keycloak",
    "Image": "quay.io/keycloak/keycloak:26.4.0",
    "Args": [
      "start-dev",
      //"export",
      //"--dir",
      //"/tmp",
      //"--users",
      //"same_file",
      "--log-console-color=true",
      //"--log=console",
      //"--db-debug-jpql=true",
      //"--http-metrics-histograms-enabled=true",
      //"--health-enabled=true",
      //"--metrics-enabled=true"
    ],
    "AdminUsername": "admin",
    "AdminPassword": "admin",
    "DatabaseType": "postgres",
    "DatabaseURL": null,
    "DatabaseUsername": "postgres",
    "DatabasePassword": "Postgres10",
    "HostPort": 8080,
    "ContainerPort": 8080,
    "Providers": [
      "./../../extensions/keycloakify-blazor-management-1.0.0.jar",
      "./../../extensions/keycloak-spis-all-1.0.0.jar"
    ]
  },
  "KeycloakDatabase": {
    "ContainerName": "keycloak-postgres",
    "Image": "postgres:16-alpine",
    "User": "postgres",
    "Password": "Postgres10",
    "DatabaseName": "keycloak-db",
    "HostPort": 5432,
    "ContainerPort": 5432,
    "VolumePath": "/var/lib/postgresql/data",
    "URL": "locahost"
  },
  "Maildev": {
    "ContainerName": "maildev",
    "Image": "maildev/maildev",
    "Ports": {
      "HostSmtp": 1025,
      "HostWeb": 1080,
      "ContainerSmtp": 1025,
      "ContainerWeb": 1080
    }
  }
}
